Texture2D resource;
SamplerState ss;

cbuffer PSCBuffer
{
    float _padding;
    float _padding1;
    float _padding2;
    float _padding3;
};

struct PSInput
{
	float4 position: SV_POSITION;
	float2 texcoord: TEXCOORD;
	float4 normal: NORMAL;
};

float4 main(PSInput input) : SV_TARGET
{
	return resource.Sample(ss, input.texcoord);
}